{
    "configurations": [
        {
            "name": "Linux",
            "includePath": [
                "/usr/local/cuda/include/",
                "${workspaceFolder}/**",
                "${workspaceFolder}/../../../Common"
            ],
            "defines": [
                "__CUDA_ARCH__=860"
            ],
            "compilerPath": "/usr/bin/gcc",
            "cStandard": "gnu17",
            "cppStandard": "gnu++17",
            "intelliSenseMode": "linux-gcc-x64",
            "configurationProvider": "ms-vscode.makefile-tools"
        }
    ],
    "version": 4
}
